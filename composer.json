    "description": "Adds Ahalogy to your WordPress site.",
    "type": "library",
    "homepage": "https://github.com/manovotny/wp-ahalogy",
    "authors": [
        {
            "name": "AUTHOR_NAME",
            "email": "author@email.com",
            "homepage": "http://author-url.com",
